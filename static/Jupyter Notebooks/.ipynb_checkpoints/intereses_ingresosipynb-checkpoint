    "IO   = path + file\n",
    "\n",
    "sheet = 'ANUAL'\n",
    "\n",
    "data = pd.read_excel(IO, sheet_name = sheet, usecols=\"B, DT:DU\", skiprows=2, nrows=28, engine='openpyxl') # Be patient...\n",
    "data = data.dropna()\n",
    "\n",
    "data[\"YEAR\"] = data[\"YEAR\"].dt.year\n",
    "data = data.rename(columns={\"INTERESES (SIN INTRA-SECTOR PUBLICO)/INGRESOS\":\"INTERESES1\"})\n",
